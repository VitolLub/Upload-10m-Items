{
   "background": {
      "persistent": true,
      "scripts": [ "js/config.js", "js/chrome.background.js", "js/cms_configs.js", "js/auth.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon-38.png",
      "default_popup": "index.html",
      "default_title": "Amazon Publisher Studio 2.0 Beta"
   },
   "description": "This is the official Amazon Associates extension for Chrome",
   "homepage_url": "https://affiliate-program.amazon.com",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "48": "img/icon-48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Amazon Publisher Studio Extension Beta",
   "permissions": [ "storage", "tabs", "http://*/*", "https://*/*", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.700"
}
